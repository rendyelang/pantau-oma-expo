{
  "expo": {
    "name": "PantauOma",
    "slug": "pantau-oma",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icons/pantauoma.png",
    "scheme": "pantauoma",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rendielang.pantauoma"
    },
    "android": {
      "package": "com.rendielang.pantauoma",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icons/pantauoma.png",
        "backgroundImage": "./assets/icons/pantauoma.png",
        "monochromeImage": "./assets/icons/pantauoma.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "android.permission.ACCESS_COARSE_LOCATION", "android.permission.ACCESS_FINE_LOCATION"],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/pantauoma.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/pantauoma.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Izinkan PantauOma mengakses lokasi untuk memantau keamanan Lansia."
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b2fe9082-24b2-47b6-b9bb-c5edf092914b"
      }
    }
  }
}
